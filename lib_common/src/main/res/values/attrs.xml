<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="enable" format="boolean" />
    <declare-styleable name="MyRadioButton">

        <attr name="drawableWidth" format="dimension" />
        <attr name="drawableHeight" format="dimension" />
        <attr name="drawableTop" format="reference" />
        <attr name="drawableLeft" format="reference" />
        <attr name="drawableRight" format="reference" />
        <attr name="drawableBottom" format="reference" />
    </declare-styleable>

    <declare-styleable name="PieChart">
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="horizontalLineLength" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr format="boolean" name="open_scale_mode" />
        <attr format="boolean" name="canLoop" />
        <attr format="boolean" name="middle_page_cover" />
        <attr format="boolean" name="autoPlay" />
    </declare-styleable>
</resources>

